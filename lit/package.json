{
  "name": "vue-lit",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "Design system package.",
  "author": {
    "name": "Pablo PÃ©rez Chueca"
  },
  "engines": {
    "node": ">=16"
  },
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "style": "./dist-styles/index.css",
  "files": [
    "dist/*",
    "dist-styles/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js"
    },
    "./index": {
      "import": "./dist/index.es.js"
    },
    "./vue": {
      "import": "./dist/vue.es.js"
    },
    "./vite": {
      "import": "./dist/vite.es.js"
    },
    "./style.css": {
      "import": "./dist-styles/index.css",
      "require": "./dist-styles/index.css"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/index.d.ts"
      ],
      "vue": [
        "./dist/vue.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "contributes": {
    "html": {
      "customData": [
        "./web-components.html-data.json"
      ]
    },
    "css": {
      "customData": [
        "./web-components.css-data.json"
      ]
    }
  },
  "directories": {},
  "scripts": {
    "dev": "vite",
    "build": "npm run build:ts && npm run build:styles",
    "build:ts": "tsc && vite build",
    "build:styles": "sass src/tokens/index.scss dist-styles/index.css",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest src"
  },
  "dependencies": {
    "lit": "^2.7.4"
  },
  "devDependencies": {
    "@babel/types": "^7.21.5",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.6",
    "jsdom": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.1",
    "vue": "^3.3.4"
  }
}